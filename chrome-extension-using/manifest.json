{
  "name": "Calfit",
  "description": "Keep yourself fit and productive by taking breaks based off your calendar schedule.",
  "manifest_version": 2,
  "version": "0.1",
  "app": {
    "background": {
      "scripts": ["background.js", "assets/jquery.min.js"]
    }
  },
  "permissions": ["gcm", "storage", "notifications", "http://calfit.azurewebsites.net/"],
  "icons": { "128": "CalFit_logo.jpg" },
 "web_accessible_resources": [
    "assets/img/*.jpg",
    "*.jpg"
  ]

}
